{
  "name": "asp",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "description": "A tool to make downloading YouTube videos easy",
  "author": {
    "name": "logie",
    "email": "hello@logie.lol"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"next dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "rimraf out && cross-env NODE_ENV=production next build",
    "build:fast": "cross-env NODE_ENV=production next build --no-lint",
    "only-build": "next build",
    "electron": "cross-env NODE_ENV=production electron .",
    "pack": "npm run build:fast && electron-builder --dir",
    "pack:fast": "npm run build:fast && electron-builder --dir --publish=never",
    "dist": "npm run build && electron-builder",
    "dist:fast": "npm run build:fast && electron-builder --publish=never"
  },
  "dependencies": {
    "@codingame/monaco-jsonrpc": "^0.4.1",
    "@codingame/monaco-languageclient": "^0.17.4",
    "@heroui/scroll-shadow": "^2.3.11",
    "@heroui/system": "^2.4.13",
    "@heroui/theme": "^2.4.13",
    "@minoru/react-dnd-treeview": "^3.5.2",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-context-menu": "^2.2.12",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "ffmpeg-static": "^5.2.0",
    "framer-motion": "^12.6.5",
    "lucide-react": "^0.487.0",
    "monaco-editor": "^0.52.2",
    "monaco-editor-core": "^0.52.2",
    "monaco-languageclient": "^9.5.0",
    "motion": "^12.6.3",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "reconnecting-websocket": "^4.4.0",
    "serve-handler": "^6.1.6",
    "smooth-scrollbar": "^8.8.4",
    "sonner": "^2.0.3",
    "stylua-wasm": "^0.1.4",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "undici": "^7.10.0",
    "vscode-ws-jsonrpc": "^3.4.0",
    "yt-dlp-exec": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4",
    "ts-loader": "^9.5.2",
    "typescript": "^5",
    "wait-on": "^8.0.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "build": {
    "appId": "com.asp.app",
    "productName": "asp",
    "directories": {
      "output": "app-dist",
      "buildResources": "build"
    },
    "files": [
      "out/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "ffmpeg",
        "to": "ffmpeg"
      },
      {
        "from": "./node_modules/yt-dlp-exec/bin",
        "to": "yt-dlp-exec/bin"
      },
      {
        "from": "./node_modules/ffmpeg-static",
        "to": "ffmpeg-static"
      }
    ],
    "asarUnpack": [
      "resources/ffmpeg/**/*",
      "resources/yt-dlp-exec/**/*",
      "resources/ffmpeg-static/**/*"
    ],
    "asar": true,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "deleteAppDataOnUninstall": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "asp"
    }
  }
}
